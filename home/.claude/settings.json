{
  "$schema": "https://json.schemastore.org/claude-code-settings.json",
  "alwaysThinkingEnabled": true,
  "feedbackSurveyState": {
    "lastShownTime": 1754076483100
  },
  "hooks": {
    "Notification": [
      {
        "hooks": [
          {
            "command": "afplay ~/.claude/sounds/gow_active_reload.mp3",
            "type": "command"
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "command": "afplay ~/.claude/sounds/gow_active_reload.mp3",
            "type": "command"
          }
        ]
      }
    ]
  },
  "model": "opus",
  "permissions": {
    "allow": [
      "Bash(cat:*)",
      "Bash(fd:*)",
      "Bash(fzf:*)",
      "Bash(grep:*)",
      "Bash(ls:*)",
      "Bash(rg:*)",
      "Bash(tree:*)",
      "WebFetch(domain:github.com)",
      "WebFetch(domain:grep.app)"
    ],
    "deny": []
  }
}
